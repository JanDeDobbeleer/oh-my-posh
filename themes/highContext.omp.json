{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "background": "#002835",
          "foreground": "#00c983",
          "leading_diamond": "\ue0b6",
          "style": "diamond",
          "template": "{{ .Icon }} {{ .HostName }} ",
          "trailing_diamond": "\ue0b0",
          "type": "os"
        },
        {
          "background": "#1e3d59",
          "foreground": "#8FD6FF",
          "powerline_symbol": "\ue0b0",
          "style": "diamond",
          "template": " \uf120 {{ .Shell }} ",
          "trailing_diamond": "\ue0b0",
          "type": "shell"
        },
        {
          "background": "#776750",
          "foreground": "#f0f1c7",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "folder_icon": "\uf115",
            "folder_separator_icon": "\\",
            "home_icon": "\uf015",
            "style": "full"
          },
          "style": "diamond",
          "template": " <#f0f1c7>\uf115 \u279c</> {{ .Path }} ",
          "trailing_diamond": "\ue0b0",
          "type": "path"
        },
        {
          "background": "#C98064",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "branch_icon": " <#ffffff>\ue0a0 </>",
            "fetch_status": false,
            "fetch_upstream_icon": true
          },
          "style": "powerline",
          "template": " \u279c ({{ .UpstreamIcon }}{{ .HEAD }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }}) ",
          "type": "git"
        },
        {
          "background": "#5E8F52",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "template": " \ue718 {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }} ",
          "type": "node"
        },
       {
         "background": "#353C69",
         "foreground": "#C6CBFF",
         "powerline_symbol": "\ue0b0",
         "style": "powerline",
         "template": " \uf073 {{ .CurrentDate | date \"02 Jan, Monday at \uf017 03:04 PM\" }} ",
         "type": "time"
       },
        {
          "background": "#5C1A47",
          "foreground": "#F2AEC9",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "template": " \uf240 {{ .Percentage }}% ",
          "type": "battery"
        },
        {
          "background": "#563667",
          "foreground": "#D8B4FF",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "always_enabled": true
          },
          "style": "powerline",
          "template": " \uf2f2 {{ .FormattedMs }} ",
          "type": "executiontime"
        },
        {
          "background": "#00b069",
          "background_templates": [
            "{{ if gt .Code 0 }}red{{ end }}"
          ],
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "always_enabled": true
          },
          "style": "diamond",
          "template": " {{ if gt .Code 0 }}\uf00d{{ else }}\uf00c{{ end }} ",
          "trailing_diamond": "\ue0b4",
          "type": "status"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#a44a33",
          "style": "plain",
          "template": " ",
          "type": "root"
        },
        {
          "foreground": "#A3345F",
          "style": "plain",
          "template": " <#36BDA1><b>\u26a1</b></><b>{{ .UserName }}</b><#1EA8B7>\u276f</><#36BDA1>\u276f</> $",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 3
}