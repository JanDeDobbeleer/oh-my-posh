{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#ffffff",
          "properties": {
            "template": "<#C591E8>\u276f</><#69FF94>\u276f</>"
          },
          "style": "plain",
          "type": "text"
        },
        {
          "foreground": "#56B6C2",
          "properties": {
            "style": "folder",
            "template": " {{ .Path }} "
          },
          "style": "plain",
          "type": "path"
        },
        {
          "foreground": "#D0666F",
          "properties": {
            "branch_icon": "",
            "fetch_status": false,
            "template": "<#5FAAE8>git:(</>{{ .HEAD }}<#5FAAE8>)</>"
          },
          "style": "plain",
          "type": "git"
        },
        {
          "foreground": "#DCB977",
          "properties": {
            "template": " \uf119"
          },
          "style": "plain",
          "type": "exit"
        },
        {
          "foreground": "#66F68F",
          "properties": {
            "template": " {{ .CurrentDate | date .Format }} ",
            "time_format": "15:04"
          },
          "style": "plain",
          "type": "time"
        }
      ],
      "type": "prompt"
    }
  ],
  "version": 1
}
