{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#ffffff",
          "properties": {
            "template": "<#C591E8>\u276f</><#69FF94>\u276f</> "
          },
          "style": "plain",
          "type": "text"
        },
        {
          "foreground": "#56B6C2",
          "properties": {
            "style": "folder",
            "template": "{{ .Path }} "
          },
          "style": "plain",
          "type": "path"
        },
        {
          "foreground": "#D0666F",
          "properties": {
            "branch_icon": "",
            "template": "<#5FAAE8>git:(</>{{ .HEAD }}<#5FAAE8>)</>"
          },
          "style": "plain",
          "type": "git"
        },
        {
          "foreground": "#DCB977",
          "properties": {
            "template": " \uf119 "
          },
          "style": "plain",
          "type": "exit"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "foreground": "#ffffff",
          "properties": {
            "command": "git log --pretty=format:%cr -1 || date +%H:%m:%S",
            "shell": "bash",
            "template": " {{ .Output }} "
          },
          "style": "plain",
          "type": "command"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#ffffff",
          "properties": {
            "template": "\uf441 "
          },
          "style": "plain",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "version": 1
}
